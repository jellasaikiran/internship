select * from film where rating = "PG-13" and film_id in ( select film_id from film_category where category_id in (select category_id from category where name = "Comedy"))


#q2

select title,count(title) from film_list,inventory,rental where film_list.category = "Horror" and film_list.FID = inventory.film_id and inventory.inventory_id = rental.inventory_id group by title order by count(title) desc limit 3;

'PULP BEVERLY', '30'
'FAMILY SWEET', '29'
'SWARM GOLD', '27'


#q3

select * from customer_list where country="india" and id in (select rental.customer_id from rental,inventory,film_list where rental.inventory_id=inventory.inventory_id and inventory.film_id=film_list.fid and film_list.category='sports');


#q4


select * from customer_list where country="Canada" and id in (select rental.customer_id from rental,inventory,film_actor,actor where rental.inventory_id=inventory.inventory_id and inventory.film_id=film_actor.film_id and film_actor.actor_id = actor.actor_id and actor.first_name='Nick' and actor.last_name = "Wahlberg");

'463', 'DARRELL POWER', '1844 Usak Avenue', '84461', '164414772677', 'Halifax', 'Canada', 'active', '2'
'189', 'LORETTA CARPENTER', '891 Novi Sad Manor', '5379', '247646995453', 'Oshawa', 'Canada', 'active', '1'


#q5


select count(*) from film_list where actors like "%SEAN WILLIAMS%";

